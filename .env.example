# Variables d'environnement pour Emploi Romand

# Supabase Configuration (OBLIGATOIRE)
VITE_SUPABASE_URL=your_supabase_project_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# Optional: OpenAI pour l'assistant IA conversationnel
VITE_OPENAI_API_KEY=your_openai_api_key

# Optional: Resend pour l'envoi d'emails
VITE_RESEND_API_KEY=your_resend_api_key

# Optional: Configuration de d√©veloppement
VITE_APP_ENV=development
VITE_DEBUG_MODE=true

# Optional: Analytics
VITE_GOOGLE_ANALYTICS_ID=your_ga_id
VITE_HOTJAR_ID=your_hotjar_id

# Optional: Sentry pour le monitoring d'erreurs
VITE_SENTRY_DSN=your_sentry_dsn

# Optional: Configuration N8N pour l'automatisation
VITE_N8N_WEBHOOK_URL=your_n8n_webhook_url